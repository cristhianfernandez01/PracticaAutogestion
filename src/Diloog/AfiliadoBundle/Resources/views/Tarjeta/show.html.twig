{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Tarjeta</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Numerotarjeta</th>
                <td>{{ entity.numeroTarjeta }}</td>
            </tr>
            <tr>
                <th>Descripciontarjeta</th>
                <td>{{ entity.descripcionTarjeta }}</td>
            </tr>
            <tr>
                <th>Vencimiento</th>
                <td>{{ entity.vencimiento }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('tarjeta') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('tarjeta_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
